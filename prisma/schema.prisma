datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Wishlist {
  id          Int      @id @default(autoincrement())
  title       String
  description String?
  createdAt   DateTime @default(now())
  done        Boolean  @default(false)
  priority    Boolean  @default(false)
  category    String?  @default("Umum")
}